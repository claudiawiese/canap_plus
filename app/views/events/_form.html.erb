<%= simple_form_for([@user, @event]) do |f| %>

  <%= f.input :game %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :date %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :capacity %>
  <%= f.input :fee %>

  <%= f.button :submit, "Add your event" %>
<% end %>
